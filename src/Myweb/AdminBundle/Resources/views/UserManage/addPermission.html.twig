{% extends "MywebAdminBundle:Layout:dashbord.html.twig"%}
{% set pageTitle= '添加权限'%}
{% block main %}
<div class="main_form">
<form role="form" action="{{path('myweb_admin_doAddPermission')}}" method="post">
  <div class="form-group">
    <label for="exampleInputEmail1">Permission</label>
    <input type="text" class="form-control" name="permission"  placeholder="Enter Permission">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Description</label>
    <textarea name="description" id="exampleInputPassword1" placeholder="Enter Desrcrition"></textarea>
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
</div>
{% endblock %}